{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Oidc": {
    "Authority": "https://localhost:5000/",
    "ClientId": "blazor",
    "RedirectUri": "https://localhost:5010/authentication/login-callback",
    "PostLogoutRedirectUri": "https://localhost:5010/authentication/logout-callback",
    "ResponseType": "code",
    "DefaultScopes": [ "openid", "profile", "email" ]
  }
}